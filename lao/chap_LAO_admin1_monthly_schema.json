      }
    },
    {
      "name": "location_name",
      "type": "string",
      "unit": null,
      "description": "Admin unit name from boundary dataset.",
      "source": {
        "provider": "OCHA COD-AB / HDX (Laos)",
        "url": "https://data.humdata.org/dataset/cod-ab-lao",
        "name_field": "shapeName"
      }
    },
    {
      "name": "rainfall",
      "type": "number",
